#include <cstdio>
#include <string>

using namespace std;

const int N = 200;

int max( int a, int b ) {
    if ( a > b ) {
        return a;
    }
    return b;
}

string toString( int n ) {
    int i, key;
    char str[ N ];
    string str1 = "";

    i = N - 1;
    while ( n > 0 ) {
        str[ i ] = n%10 + '0';
        n /= 10;
        --i;
    }

    key = 0;
    for ( i = i + 1; i < N; ++i ) {
        str[ key ] = str[ i ];
        ++key;
    }
    str[ key ] = '\0';

    str1 = str;

    return str1;
}

string add( string a, string b ) {
    int i, sum, length;
    bool left;

    length = max( (int)a.size(), (int)b.size() );
    string c = "";
    if ( length == (int)a.size() ) {
        i = length - (int)b.size();
        while ( i > 0 ) {
            b.insert( 0, "0" );
            --i;
        }
    }
    else {
        i = length - (int)a.size();
        while ( i > 0 ) {
            a.insert( 0, "0" );
            --i;
        }
    }

    printf( "%s\n", b.c_str() );

    for ( i = length - 1; i >= 0; --i ) {
        sum = ( a[ i ] - '0' ) + ( b[ i ] - '0' );
        if ( sum > 9 ) {
            c.insert( 0, toString( ( sum % 10 ) + left ) ); 
            left = true;
        }
        else {
            c.insert( 0, toString( sum + left ) );
            left = false;
        }
        printf( "%s\n", c.c_str() );
    }
    return c;
}

string str = "52338";
string str1 = "00456";
int a = 856;

int main() {
    printf( "%s\n", add( str, str1 ).c_str() );
    return 0;
}
